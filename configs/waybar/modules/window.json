{
    "hyprland/window": {
        "format": "{title}",
        "max-length": 40,
        "separate-outputs": true,
        "rewrite": {
            "(.*) — firefox": "󰈹 $1",
            "(.*)firefox": "󰈹 Firefox",
            "(.*) - zsh": "> [$1]",
            "^.*~$": "󰄛 Kitty $1",
            "(.*) - mpv": "Mpv",
            "^.*v( .*|$)": " $1",
            "^.*zathura.*": "Zathura $1",
            "(.*)Visual Studio Code": "Code 󰨞"
        }
    }
}
